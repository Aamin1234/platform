{
  "linters" : {
    "jshint" : {
      "type" : "jshint",
      "include" : "(\\.js$)",
      "exclude" : "@/libs/.*\\.js$@",
      "jshint.jshintrc" : ".jshintrc",
      "jshint.jshintignore" : ".jshintignore"
    },
    "phpcs" : {
      "type" : "phpcs",
      "include" : "(\\.php$)",
      "exclude" : "@^(application|modules|plugins)/.*@"
    }
  }
}
